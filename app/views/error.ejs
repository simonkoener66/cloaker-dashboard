<!doctype html>
<html>

<head>
	<link rel="stylesheet" href="/bower_components/angular-material/angular-material.min.css">
	<link rel="stylesheet" href="/styles/bootstrap.css">
    <link rel="stylesheet" href="/styles/ui.css">
    <link rel="stylesheet" href="/styles/main.css">
	<title>Server Error</title>
</head>

<body class="app ng-scope body-wide body-err">
	<div class="page-err" style="margin: auto">
	    <div class="err-container text-center">
	        <div class="err">
	            <h1>500</h1>
	            <h2>Sorry, server error occurred.</h2>
	        </div>

	        <div class="err-body">
	            <a ui-wave href="#/" class="btn btn-raised btn-lg btn-default">
	                Go Back to Home Page
	            </a>
	        </div>

	        <div class="err" style="max-width: 900px; margin: auto" ng-show="error"><%= error.stack %></div>
	    </div>
	</div>
</body>

</html>